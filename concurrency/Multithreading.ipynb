   "source": [
    "from time import sleep, perf_counter\n",
    "\n",
    "def task():\n",
    "    print('Starting a tast')\n",
    "    sleep(1)\n",
    "    print('Done')\n",
    "\n",
    "start = perf_counter()\n",
    "task()\n",
    "task()\n",
    "end = perf_counter()\n",
    "print(f\"It's take {end - start:.2f} second(s) to completes all the tasks\")"
